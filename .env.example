# coding-agent environment configuration
# copy this file to .env and fill in your values

# =============================================================================
# llm provider api keys (at least one required)
# =============================================================================

# anthropic (claude)
ANTHROPIC_API_KEY=

# openai (gpt-4, etc.)
OPENAI_API_KEY=

# together ai
TOGETHER_API_KEY=

# google (gemini)
GOOGLE_API_KEY=
# alternative: GEMINI_API_KEY=

# =============================================================================
# tool api keys (optional)
# =============================================================================

# tavily for web search tool
TAVILY_API_KEY=

# =============================================================================
# agent configuration (optional)
# =============================================================================

# llm provider to use: anthropic, openai, together, google
# if not set, auto-detects based on available api keys
LLM_PROVIDER=

# logging level: DEBUG, INFO, WARNING, ERROR
CODING_AGENT_LOG_LEVEL=WARNING
